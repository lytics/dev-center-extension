{
  "name": "lytics-dev-tools",
  "version": "0.0.0",
  "title": "Lytics Dev Tools (Alpha)",
  "description": "Chrome extension for exploring and debugging Lytics web SDK and APIs.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lytics/dev-center-extension"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "yarn build:hmr && (run-p wss build:watch)",
    "dev:firefox": "yarn build:hmr && (run-p wss build:firefox:watch)",
    "test": "vitest",
    "commitlint": "commitlint --edit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.1",
    "@mui/styles": "^5.15.2",
    "@mui/x-charts": "^6.18.4",
    "construct-style-sheets-polyfill": "3.1.0",
    "date-fns": "^3.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-json-tree": "^0.18.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.21.1",
    "react-server-dom-webpack": "^0.0.1",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@rollup/plugin-typescript": "11.1.5",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.251",
    "@types/jest": "^29.5.11",
    "@types/node": "20.8.10",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.14",
    "@types/ws": "8.5.8",
    "@typescript-eslint/eslint-plugin": "6.18.0",
    "@typescript-eslint/parser": "6.9.1",
    "@vitejs/plugin-react": "4.2.0",
    "chokidar": "3.5.3",
    "cross-env": "7.0.3",
    "eslint": "8.53.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "jsdom": "^22.1.0",
    "lint-staged": "15.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.0",
    "rollup": "4.3.0",
    "sass": "1.69.5",
    "ts-loader": "9.5.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-static-copy": "^1.0.0",
    "vitest": "^1.1.1",
    "ws": "8.14.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "yarn@1.22.19"
}
